local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer

local enabled1 = false
local enabled2 = false

-- GUI
local screenGui = player.PlayerGui:FindFirstChild("AutoPanelGui") or Instance.new("ScreenGui")
screenGui.Name = "AutoPanelGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = player.PlayerGui

-- Container
local container = Instance.new("Frame")
container.Size = UDim2.new(0, 220, 0, 240)
container.Position = UDim2.new(0.5, -110, 0.1, 0)
container.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
container.BorderSizePixel = 0
container.Parent = screenGui
Instance.new("UICorner", container)

-- Vi·ªÅn
local stroke = Instance.new("UIStroke", container)
stroke.Thickness = 3
stroke.Color = Color3.fromRGB(255,50,50)

-- Title
local title = Instance.new("TextLabel", container)
title.Size = UDim2.new(1,0,0,30)
title.BackgroundColor3 = Color3.fromRGB(50,50,50)
title.Text = "AUTO PANEL"
title.TextColor3 = Color3.fromRGB(0,255,100)
title.TextScaled = true
title.Font = Enum.Font.GothamBold
Instance.new("UICorner", title)

-- ScrollFrame
local scrollFrame = Instance.new("ScrollingFrame", container)
scrollFrame.Size = UDim2.new(1,0,1,-30)
scrollFrame.Position = UDim2.new(0,0,0,30)
scrollFrame.BackgroundTransparency = 1
scrollFrame.BorderSizePixel = 0
scrollFrame.ScrollBarThickness = 6
scrollFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
scrollFrame.CanvasSize = UDim2.new(0,0,0,0)

-- UIListLayout (t·ª± cƒÉn button)
local list = Instance.new("UIListLayout", scrollFrame)
list.Padding = UDim.new(0, 10)
list.SortOrder = Enum.SortOrder.LayoutOrder

-- MAKE BUTTON FUNCTION (AUTO)
local buttonIndex = 0
local function makeBtn(text, color)
    buttonIndex += 1

    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.9,0,0,40)
    btn.BackgroundColor3 = color or Color3.fromRGB(40,40,40)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Text = text
    btn.TextScaled = true
    btn.Font = Enum.Font.GothamBold
    btn.LayoutOrder = buttonIndex
    btn.Parent = scrollFrame

    Instance.new("UICorner", btn)

    local s = Instance.new("UIStroke", btn)
    s.Thickness = 3
    s.Color = Color3.fromRGB(255,50,50)

    return btn
end

-- Buttons
