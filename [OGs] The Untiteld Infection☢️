local TARGET_NAME = "TransfurPart"  -- üî• ƒê·ªîI T√äN T·∫†I ƒê√ÇY

local workspace = game:GetService("Workspace")
local count = 0

for _, obj in ipairs(workspace:GetDescendants()) do
    if obj:IsA("BasePart") and obj.Name == TARGET_NAME then
        obj:Destroy()
        count += 1
    end
end

print("ƒê√£ x√≥a t·ªïng c·ªông:", count, "part t√™n", TARGET_NAME)
