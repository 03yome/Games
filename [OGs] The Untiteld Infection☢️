-------------------------------
-- Auto loops
-------------------------------
task.spawn(function()
    while task.wait(0.1) do
        if enabled1 then
            ReplicatedStorage.EventsSJW.Rush:FireServer()
        end
    end
end)

task.spawn(function()
    while task.wait(0.1) do
        if enabled2 then
            ReplicatedStorage.AddGamePassCash:FireServer()
        end
    end
end)

-------------------------------
-- Toggle events
-------------------------------
toggle1.MouseButton1Click:Connect(function()
    enabled1 = not enabled1
    if enabled1 then
        toggle1.Text = "Rush: ON"
        toggle1.BackgroundColor3 = Color3.fromRGB(0,255,100)
    else
        toggle1.Text = "Rush: OFF"
        toggle1.BackgroundColor3 = Color3.fromRGB(255,50,50)
    end
end)

toggle2.MouseButton1Click:Connect(function()
    enabled2 = not enabled2
    if enabled2 then
        toggle2.Text = "Add cash: ON"
        toggle2.BackgroundColor3 = Color3.fromRGB(0,255,100)
    else
        toggle2.Text = "Add cash: OFF"
        toggle2.BackgroundColor3 = Color3.fromRGB(255,50,50)
    end
end)

