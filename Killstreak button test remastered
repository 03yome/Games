local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Killstreak button test remastered")

local KillingCheats = PhantomForcesWindow:NewSection("main")

KillingCheats:CreateButton("Aura hacker unfinished ", function()
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "hack"          -- Ä‘áº·t tÃªn tÃ¹y Ã½
ScreenGui.ResetOnSpawn = false          -- khÃ´ng máº¥t khi respawn
ScreenGui.Parent = game:GetService("CoreGui")

local ToggleBtn = Instance.new("TextButton", ScreenGui)
ToggleBtn.Size = UDim2.new(0,150,0,40)
ToggleBtn.Position = UDim2.new(0.1,0,0.2,0)
ToggleBtn.Text = "TheForce: OFF"
ToggleBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
ToggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
ToggleBtn.Active = true
ToggleBtn.Draggable = true

local Auto = false
ToggleBtn.MouseButton1Click:Connect(function()
    Auto = not Auto
    ToggleBtn.Text = Auto and "Aura beta: ON" or "Aura beta: OFF"
    ToggleBtn.BackgroundColor3 = Auto and Color3.fromRGB(0,170,0) or Color3.fromRGB(50,50,50)
end)

-- Auto loop
local remote = game:GetService("ReplicatedStorage"):WaitForChild("AbilityEvents"):WaitForChild("Shockwave")
task.spawn(function()
    while wait(1) do
        if Auto then
            pcall(function()
                remote:FireServer()
            end)
        end
    end
end)
print("HI")
end)

