local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("Killstreak button test remastered")

local KillingCheats = PhantomForcesWindow:NewSection("main")
KillingCheats:CreateButton("Aura Hacker", function()
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("TheForce")
local Player = game.Players.LocalPlayer

-- GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = Player:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false  -- âœ… GUI sáº½ khÃ´ng máº¥t khi nhÃ¢n váº­t cháº¿t

local ToggleBtn = Instance.new("TextButton", ScreenGui)
ToggleBtn.Size = UDim2.new(0,150,0,40)
ToggleBtn.Position = UDim2.new(0.1,0,0.2,0)
ToggleBtn.Text = "TheForce: OFF"
ToggleBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
ToggleBtn.TextColor3 = Color3.fromRGB(255,255,255)
ToggleBtn.Draggable = true

local Auto = false
ToggleBtn.MouseButton1Click:Connect(function()
    Auto = not Auto
    ToggleBtn.Text = Auto and "Aura: ON" or "Aura: OFF"
    ToggleBtn.BackgroundColor3 = Auto and Color3.fromRGB(0,170,0) or Color3.fromRGB(50,50,50)
end)

-- Auto loop
task.spawn(function()
    while wait(1) do
        if Auto then
            pcall(function()
                remote:FireServer()
            end)
        end
    end
end)


print("HI")
end)

KillingCheats:CreateButton("Haki Hacker", function()
game:GetService("ReplicatedStorage"):WaitForChild("KillstreakEvents"):WaitForChild("Haki"):FireServer()
print("HI")
end)

