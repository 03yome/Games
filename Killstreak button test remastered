KillingCheats:CreateButton("delete button", function()
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui")

local guiName = "hack" -- <-- Ä‘á»•i tÃªn GUI cáº§n xÃ³a

-- XÃ³a trong PlayerGui
local playerGui = Player:FindFirstChild("PlayerGui")
if playerGui then
    local gui = playerGui:FindFirstChild(guiName)
    if gui then
        gui:Destroy()
        print("âœ… ÄÃ£ xÃ³a "..guiName.." trong PlayerGui")
    end
end

-- XÃ³a trong CoreGui
local coreGui = CoreGui:FindFirstChild(guiName)
if coreGui then
    coreGui:Destroy()
    print("âœ… ÄÃ£ xÃ³a "..guiName.." trong CoreGui")
end

print("HI")
end)

