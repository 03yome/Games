    while task.wait(0.1) do
        if AutoKill then
            for _, plr in ipairs(Players:GetPlayers()) do
                if plr ~= LocalPlayer and plr.Character then
                    local part = plr.Character:FindFirstChild(selectedPart)
                    if part then
                        pcall(function()
                            remote:FireServer(part)
                        end)
                    end
                end
            end
        end
    end
end)

print("HI")
end)

KillingCheats:CreateButton("give kill", function()
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill")

-- üìå T·∫°o GUI
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer

-- üìå T·∫°o GUI
local gui = Instance.new("ScreenGui")
gui.Name = "hack"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

-- üìå T·∫°o Toggle Button
local ToggleBtn = Instance.new("TextButton", gui)
ToggleBtn.Size = UDim2.new(0, 150, 0, 40)
ToggleBtn.Position = UDim2.new(0.1, 0, 0.2, 0)
ToggleBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleBtn.Text = "GiveKill: OFF"
ToggleBtn.Active = true
ToggleBtn.Draggable = true

-- üìå Tr·∫°ng th√°i auto loop
local AutoLoop = false

-- üîò B·∫•m ƒë·ªÉ ƒë·ªïi tr·∫°ng th√°i
ToggleBtn.MouseButton1Click:Connect(function()
    AutoLoop = not AutoLoop
    ToggleBtn.Text = AutoLoop and "GiveKill: ON" or "GiveKill: OFF"
    ToggleBtn.BackgroundColor3 = AutoLoop and Color3.fromRGB(0,170,0) or Color3.fromRGB(30,30,30)
end)

-- üî• LOOP GIVEKILL
task.spawn(function()
    while true do
        if AutoLoop then
            pcall(function()
                remote:FireServer()
            end)
        end
        wait(0.1) -- t·ªëc ƒë·ªô spam
    end
end)


print("HI")
end)

KillingCheats:CreateButton("autokill OG", function()
local plr = game.Players.LocalPlayer
local rs = game:GetService("ReplicatedStorage")
local slapFolder = rs:WaitForChild("SlapRemotes")

-- GUI ch√≠nh
local gui = Instance.new("ScreenGui")
gui.Name = "hack"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 250, 0, 150)
frame.Position = UDim2.new(0.4,0,0.3,0)
frame.BackgroundColor3 = Color3.fromRGB(40,40,40)
frame.Active = true
frame.Draggable = true

-- Title
local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1,0,0,25)
title.BackgroundColor3 = Color3.fromRGB(30,30,30)
title.Text = "Auto Kill Menu"
title.TextColor3 = Color3.new(1,1,1)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 18

-- Dropdown button
local dropdown = Instance.new("TextButton", frame)
dropdown.Size = UDim2.new(1,-20,0,30)
dropdown.Position = UDim2.new(0,10,0,35)
dropdown.BackgroundColor3 = Color3.fromRGB(60,60,60)
dropdown.TextColor3 = Color3.new(1,1,1)
dropdown.Text = "Ch·ªçn Kill Remote"

-- Scrollable list
local listFrame = Instance.new("ScrollingFrame", frame)
listFrame.Size = UDim2.new(1,-20,0,80)
listFrame.Position = UDim2.new(0,10,0,70)
listFrame.BackgroundColor3 = Color3.fromRGB(25,25,25)
listFrame.Visible = false
listFrame.ScrollBarThickness = 6
listFrame.CanvasSize = UDim2.new(0,0,0,0)
listFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y

local UIList = Instance.new("UIListLayout", listFrame)
UIList.SortOrder = Enum.SortOrder.LayoutOrder
UIList.Padding = UDim.new(0,2)

-- T·∫°o n√∫t remote
local chosenRemote = nil
for _, remote in ipairs(slapFolder:GetChildren()) do
    if remote:IsA("RemoteEvent") then
        local btn = Instance.new("TextButton", listFrame)
        btn.Size = UDim2.new(1,0,0,30)
        btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
        btn.TextColor3 = Color3.new(1,1,1)
        btn.Font = Enum.Font.SourceSans
        btn.TextSize = 18
        btn.TextWrapped = true -- wrap text d√†i
        btn.TextXAlignment = Enum.TextXAlignment.Left
        btn.Text = remote.Name

        btn.MouseButton1Click:Connect(function()
            dropdown.Text = "Remote: "..remote.Name
            chosenRemote = remote
            listFrame.Visible = false
        end)
    end
end

-- Hi·ªán/·∫©n list
dropdown.MouseButton1Click:Connect(function()
    listFrame.Visible = not listFrame.Visible
end)

-- Auto Kill toggle
local toggle = Instance.new("TextButton", frame)
toggle.Size = UDim2.new(1,-20,0,30)
toggle.Position = UDim2.new(0,10,0,110)
toggle.BackgroundColor3 = Color3.fromRGB(100,30,30)
toggle.TextColor3 = Color3.new(1,1,1)
toggle.Text = "Auto Kill: OFF"

local running = false
toggle.MouseButton1Click:Connect(function()
    running = not running
    toggle.Text = running and "Auto Kill: ON" or "Auto Kill: OFF"
    toggle.BackgroundColor3 = running and Color3.fromRGB(30,120,30) or Color3.fromRGB(100,30,30)

    if running then
        task.spawn(function()
            while running do
                if chosenRemote then
                    for _, pl in ipairs(game.Players:GetPlayers()) do
                        if pl ~= plr and pl.Character then
                            local hum = pl.Character:FindFirstChild("Humanoid")
                            local hrp = pl.Character:FindFirstChild("HumanoidRootPart")
                            if hum and hrp then
                                pcall(function()
                                    chosenRemote:FireServer(hum, hrp.Position, 1)
                                end)
                            end
                        end
                    end
                end
                task.wait(0.15)
            end
        end)
    end
end)

print("HI")
end)

