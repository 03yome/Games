KillingCheats:CreateButton("autokill oG target", function()
local plr = game.Players.LocalPlayer
local rs = game:GetService("ReplicatedStorage")
local slapFolder = rs:WaitForChild("SlapRemotes")

-- GUI ch√≠nh
local gui = Instance.new("ScreenGui")
gui.Name = "hack"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 260, 0, 310)
frame.Position = UDim2.new(0.35,0,0.25,0)
frame.BackgroundColor3 = Color3.fromRGB(40,40,40)
frame.Active = true
frame.Draggable = true

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1,0,0,30)
title.BackgroundColor3 = Color3.fromRGB(30,30,30)
title.Text = "Auto Kill Menu"
title.TextColor3 = Color3.new(1,1,1)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 20


---------------------------------------------------------
-- üîª REMOTE DROPDOWN
---------------------------------------------------------

local remoteBtn = Instance.new("TextButton", frame)
remoteBtn.Size = UDim2.new(1,-20,0,30)
remoteBtn.Position = UDim2.new(0,10,0,45)
remoteBtn.BackgroundColor3 = Color3.fromRGB(60,60,60)
remoteBtn.TextColor3 = Color3.new(1,1,1)
remoteBtn.Text = "Ch·ªçn Kill Remote"

local remoteList = Instance.new("ScrollingFrame", frame)
remoteList.Size = UDim2.new(1,-20,0,80)
remoteList.Position = UDim2.new(0,10,0,80)
remoteList.BackgroundColor3 = Color3.fromRGB(20,20,20)
remoteList.Visible = false
remoteList.ScrollBarThickness = 6
remoteList.AutomaticCanvasSize = Enum.AutomaticSize.Y

local UIListRemote = Instance.new("UIListLayout", remoteList)
UIListRemote.SortOrder = Enum.SortOrder.LayoutOrder
UIListRemote.Padding = UDim.new(0,3)

local chosenRemote = nil

local function loadRemotes()
	-- Ch·ªâ x√≥a TextButton, KH√îNG xo√° UIListLayout
	for _, c in ipairs(remoteList:GetChildren()) do
		if c:IsA("TextButton") then c:Destroy() end
	end

	for _, remote in ipairs(slapFolder:GetChildren()) do
		if remote:IsA("RemoteEvent") then
			local btn = Instance.new("TextButton", remoteList)
			btn.Size = UDim2.new(1,0,0,25)
			btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
			btn.TextColor3 = Color3.new(1,1,1)
			btn.Text = remote.Name
			btn.Font = Enum.Font.SourceSans
			btn.TextSize = 16

			btn.MouseButton1Click:Connect(function()
				chosenRemote = remote
				remoteBtn.Text = "Remote: " .. remote.Name
				remoteList.Visible = false
			end)
		end
	end
end

loadRemotes()

remoteBtn.MouseButton1Click:Connect(function()
	remoteList.Visible = not remoteList.Visible
end)


---------------------------------------------------------
-- üîª PLAYER DROPDOWN
---------------------------------------------------------

local playerBtn = Instance.new("TextButton", frame)
playerBtn.Size = UDim2.new(1,-20,0,30)
playerBtn.Position = UDim2.new(0,10,0,170)
playerBtn.BackgroundColor3 = Color3.fromRGB(60,60,60)
playerBtn.TextColor3 = Color3.new(1,1,1)
playerBtn.Text = "Ch·ªçn Ng∆∞·ªùi Ch∆°i"

local playerList = Instance.new("ScrollingFrame", frame)
playerList.Size = UDim2.new(1,-20,0,80)
playerList.Position = UDim2.new(0,10,0,205)
playerList.BackgroundColor3 = Color3.fromRGB(20,20,20)
playerList.Visible = false
playerList.ScrollBarThickness = 6
playerList.AutomaticCanvasSize = Enum.AutomaticSize.Y

local UIListPlayer = Instance.new("UIListLayout", playerList)
UIListPlayer.SortOrder = Enum.SortOrder.LayoutOrder
UIListPlayer.Padding = UDim.new(0,3)

local chosenPlayer = nil

local function refreshPlayers()
	for _, c in ipairs(playerList:GetChildren()) do
		if c:IsA("TextButton") then c:Destroy() end
	end

	for _, p in ipairs(game.Players:GetPlayers()) do
		if p ~= plr then
			local btn = Instance.new("TextButton", playerList)
			btn.Size = UDim2.new(1,0,0,25)
			btn.BackgroundColor3 = Color3.fromRGB(50,50,50)
			btn.TextColor3 = Color3.new(1,1,1)
			btn.Text = p.Name
			btn.Font = Enum.Font.SourceSans
			btn.TextSize = 16

			btn.MouseButton1Click:Connect(function()
				chosenPlayer = p
				playerBtn.Text = "Player: " .. p.Name
				playerList.Visible = false
			end)
		end
	end
end

playerBtn.MouseButton1Click:Connect(function()
	refreshPlayers()
	playerList.Visible = not playerList.Visible
end)


---------------------------------------------------------
-- üî• AUTO KILL BUTTON
---------------------------------------------------------

local toggle = Instance.new("TextButton", frame)
toggle.Size = UDim2.new(1,-20,0,30)
toggle.Position = UDim2.new(0,10,0,135)
toggle.BackgroundColor3 = Color3.fromRGB(120,30,30)
toggle.TextColor3 = Color3.new(1,1,1)
toggle.Text = "Auto Kill: OFF"

local running = false

toggle.MouseButton1Click:Connect(function()
	running = not running
	toggle.Text = running and "Auto Kill: ON" or "Auto Kill: OFF"
	toggle.BackgroundColor3 = running and Color3.fromRGB(30,120,30) or Color3.fromRGB(120,30,30)

	if running then
		task.spawn(function()
			while running do
				if chosenRemote and chosenPlayer and chosenPlayer.Character then
					local hum = chosenPlayer.Character:FindFirstChild("Humanoid")
					local hrp = chosenPlayer.Character:FindFirstChild("HumanoidRootPart")

					if hum and hrp then
						pcall(function()
							chosenRemote:FireServer(hum, hrp.Position, 1)
						end)
					end
				end
				task.wait(0.15)
			end
		end)
	end
end)

print("HI")
end)

KillingCheats:CreateButton("give kill2", function()
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill2")

-- üìå T·∫°o GUI
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill2")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer

-- üìå T·∫°o GUI
local gui = Instance.new("ScreenGui")
gui.Name = "hack"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

-- üìå T·∫°o Toggle Button
local ToggleBtn = Instance.new("TextButton", gui)
ToggleBtn.Size = UDim2.new(0, 150, 0, 40)
ToggleBtn.Position = UDim2.new(0.1, 0, 0.2, 0)
ToggleBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleBtn.Text = "GiveKill2: OFF"
ToggleBtn.Active = true
ToggleBtn.Draggable = true

-- üìå Tr·∫°ng th√°i auto loop
local AutoLoop = false

-- üîò B·∫•m ƒë·ªÉ ƒë·ªïi tr·∫°ng th√°i
ToggleBtn.MouseButton1Click:Connect(function()
    AutoLoop = not AutoLoop
    ToggleBtn.Text = AutoLoop and "GiveKill2: ON" or "GiveKill2: OFF"
    ToggleBtn.BackgroundColor3 = AutoLoop and Color3.fromRGB(0,170,0) or Color3.fromRGB(30,30,30)
end)

-- üî• LOOP GIVEKILL
task.spawn(function()
    while true do
        if AutoLoop then
            pcall(function()
                remote:FireServer()
            end)
        end
        wait(0.1) -- t·ªëc ƒë·ªô spam
    end
end)


print("HI")
end)

KillingCheats:CreateButton("Swap.Position.Hacker", function()
game:GetService("ReplicatedStorage"):WaitForChild("AbilityEvents"):WaitForChild("SwapPosition"):FireServer()
print("HI")
end)

KillingCheats:CreateButton("trolling player", function()
game:GetService("ReplicatedStorage"):WaitForChild("AbilityEvents"):WaitForChild("ZeroG"):FireServer()
print("HI")
end)

KillingCheats:CreateButton("MakeTheStarFall", function()
game:GetService("ReplicatedStorage").AbilityEvents.MakeTheStarFall:FireServer()
print("HI")
end)

print("HI")
end)
