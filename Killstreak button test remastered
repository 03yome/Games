KillingCheats:CreateButton("give kill", function()
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill")

-- ðŸ“Œ Táº¡o GUI
local RS = game:GetService("ReplicatedStorage")
local remote = RS:WaitForChild("AdminEvents"):WaitForChild("GiveKill")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer

-- ðŸ“Œ Táº¡o GUI
local gui = Instance.new("ScreenGui")
gui.Name = "hack"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

-- ðŸ“Œ Táº¡o Toggle Button
local ToggleBtn = Instance.new("TextButton", gui)
ToggleBtn.Size = UDim2.new(0, 150, 0, 40)
ToggleBtn.Position = UDim2.new(0.1, 0, 0.2, 0)
ToggleBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleBtn.Text = "GiveKill: OFF"
ToggleBtn.Active = true
ToggleBtn.Draggable = true

-- ðŸ“Œ Tráº¡ng thÃ¡i auto loop
local AutoLoop = false

-- ðŸ”˜ Báº¥m Ä‘á»ƒ Ä‘á»•i tráº¡ng thÃ¡i
ToggleBtn.MouseButton1Click:Connect(function()
    AutoLoop = not AutoLoop
    ToggleBtn.Text = AutoLoop and "GiveKill: ON" or "GiveKill: OFF"
    ToggleBtn.BackgroundColor3 = AutoLoop and Color3.fromRGB(0,170,0) or Color3.fromRGB(30,30,30)
end)

-- ðŸ”¥ LOOP GIVEKILL
task.spawn(function()
    while true do
        if AutoLoop then
            pcall(function()
                remote:FireServer()
            end)
        end
        wait(0.1) -- tá»‘c Ä‘á»™ spam
    end
end)


print("HI")
end)

