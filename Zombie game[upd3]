local Tab = Window:CreateTab("Server/client", nil)
local Section = Tab:CreateSection("Server")

local Button = Tab:CreateButton({
   Name = "Hack Sniper",
   Callback = function()
-- services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local lplr = Players.LocalPlayer
local char = lplr.Character or lplr.CharacterAdded:Wait()

-- equip sniper
ReplicatedStorage.Remotes.Shop.EquipWeapon:InvokeServer("Sniper")

-- wait sniper
local Sniper
repeat
    Sniper = lplr:FindFirstChild("Sniper", true) or char:FindFirstChild("Sniper", true)
    task.wait()
until Sniper and Sniper:FindFirstChild("LocalScript")

-- hook script
local scr = getsenv(Sniper.LocalScript)
local shoot = debug.getupvalue(scr.FireGun, 5)
local calc = debug.getupvalue(scr.FireGun, 6)
local sound = Sniper.Handle["Sniper fire sound"]

scr.FireGun = function(...)
    local sc = sound:Clone()
    sc.Playing = true
    sc.Parent = Sniper.Handle
    task.delay(sc.TimeLength, function()
        sc:Destroy()
    end)

    shoot(Sniper.Flash2.Position, calc(...))
end

-- minigun logic
local mouse = lplr:GetMouse()
local buttonDown = false

mouse.Button1Down:Connect(function()
    if not buttonDown and Sniper.Parent == char then
        buttonDown = true
        repeat
            coroutine.wrap(scr.FireGun)(mouse.X, mouse.Y)
            task.wait()
        until not buttonDown
    end
end)

mouse.Button1Up:Connect(function()
    buttonDown = false
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Summon Zombie",
   Callback = function()
game:GetService("ReplicatedStorage").Remotes.ZombieRelated.Necro.AbilityPlayer:FireServer()
   end,
})

local Button = Tab:CreateButton({
   Name = "place landmines",
   Callback = function()
game:GetService("ReplicatedStorage").NetworkEvents.RemoteEvent:FireServer("PLACE_LANDMINE")
   end,
})

local landmineLag = false
local landmineRemote = ReplicatedStorage:FindFirstChild("Remotes", true)

Tab:CreateToggle({
    Name = "Lag Server (Landmine Edition)",
    CurrentValue = false,
    Flag = "LandmineLag",
    Callback = function(Value)
        landmineLag = Value
        if not Value then
            rejoin()
        end
    end,
})

task.spawn(function()
    while task.wait() do
        if landmineLag and landmineRemote then
            for _ = 1, 24 do
                task.spawn(function()
                    for _ = 1, 24000 do
                        landmineRemote:FireServer()
                    end
                end)
            end
        end
    end
end)

local necroLag = false
local necroRemote =
    ReplicatedStorage.Remotes.ZombieRelated.Necro.AbilityPlayer

Tab:CreateToggle({
    Name = "Lag Server (Necro Edition)",
    CurrentValue = false,
    Flag = "NecroLag",
    Callback = function(Value)
        necroLag = Value
        if not Value then
            rejoin()
        end
    end,
})

task.spawn(function()
    while task.wait() do
        if necroLag then
            for _ = 1, 24 do
                task.spawn(function()
                    for _ = 1, 24000 do
                        necroRemote:FireServer()
                    end
                end)
            end
        end
    end
end)

local acidLag = false
local acidRemote =
    ReplicatedStorage.Remotes.ZombieRelated.AcidSpit

Tab:CreateToggle({
    Name = "Lag Server (Acid Edition)",
    CurrentValue = false,
    Flag = "AcidLag",
    Callback = function(Value)
        acidLag = Value
        if not Value then
            rejoin()
        end
    end,
})

task.spawn(function()
    while task.wait() do
        if acidLag then
            for x = -266, 378, 18 do
                for z = -268, 378, 18 do
                    acidRemote:FireServer(
                        Vector3.new(x,255,z),
                        Vector3.new()
                    )
                end
            end
        end
    end
end)

local acidRain = false

Tab:CreateToggle({
    Name = "Acid Rain",
    CurrentValue = false,
    Flag = "AcidRain",
    Callback = function(Value)
        acidRain = Value
        if not Value then
            rejoin()
        end
    end,
})

task.spawn(function()
    while task.wait(0.3) do
        if acidRain then
            for x = -266, 378, 18 do
                for z = -268, 378, 18 do
                    acidRemote:FireServer(
                        Vector3.new(x,255,z),
                        Vector3.new()
                    )
                end
            end
        end
    end
end)

local Button = Tab:CreateButton({
   Name = "Summon Acid",
   Callback = function()
local AcidRemote = game:GetService("ReplicatedStorage").Remotes.ZombieRelated.AcidSpit

for coverX = -266, 378, 18 do
    for coverZ = -268, 378, 18 do
        AcidRemote:FireServer(
            Vector3.new(coverX, 255, coverZ),
            Vector3.new(0,0,0)
        )
    end
end
   end,
})

do
    local tgl
    local rStor = game:GetService("ReplicatedStorage")
    local lplr = game.Players.LocalPlayer
    local char = lplr.Character or lplr.CharacterAdded:Wait()

    local event = rStor.Remotes.ZombieRelated.PlayerAttack
    local eventC = 0
    local Attack = rStor.Attack:Clone()

    tgl = Tab:CreateToggle({
        Name = "Annoy all",
        CurrentValue = false,
        Flag = "AnnoyAll",
        Callback = function(c)
            -- ƒë·∫£m b·∫£o c√≥ tool Attack
            if not lplr:FindFirstChild("Attack", true) and not char:FindFirstChild("Attack", true) then
                Attack.Parent = lplr:WaitForChild("Backpack")
            end

            if c then
                task.spawn(function()
                    while tgl.CurrentValue do
                        if find.Weapon(true) then
                            for _, v in living:GetChildren() do
                                if v ~= lplr.Character then
                                    local hum = v:FindFirstChildWhichIsA("Humanoid")
                                    local root = v:FindFirstChild("HumanoidRootPart")

                                    if hum and root and hum.Health > 0 then
                                        if eventC < 100 then
                                            task.spawn(function()
                                                eventC += 1
                                                event:InvokeServer(root)
                                                eventC -= 1
                                            end)
                                        end
                                    end
                                end
                            end
                        end
                        task.wait()
                    end
                end)
            end
        end
    })
end


local Section = Tab:CreateSection("Client")

local Button = Tab:CreateButton({
   Name = "Unload Entities(P)",
   Callback = function()
-- services
local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")

local lplr = Players.LocalPlayer
local char = lplr.Character or lplr.CharacterAdded:Wait()

-- folder entity (ƒë·ªïi n·∫øu game kh√°c)
local living = workspace:FindFirstChild("LivingThings")
if not living then
    warn("LivingThings not found")
    return
end

local unloaded = false

-- lu√¥n gi·ªØ character trong workspace
local function keepCharacterSafe()
    if char and char.Parent ~= workspace then
        char.Parent = workspace
    end
end

-- theo d√µi respawn
lplr.CharacterAdded:Connect(function(c)
    char = c
    task.wait()
    keepCharacterSafe()
end)

-- toggle unload
local function toggleUnload()
    unloaded = not unloaded
    keepCharacterSafe()

    if unloaded then
        living.Parent = nil
        warn("Unload Entities: ON")
    else
        living.Parent = workspace
        warn("Unload Entities: OFF")
    end
end

-- ph√≠m b·∫≠t/t·∫Øt
UIS.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == Enum.KeyCode.P then
        toggleUnload()
    end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Remove Barrires",
   Callback = function()
-- x√≥a barrier trong workspace
for _, name in ipairs(TeamBarriers) do
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj.Name == name then
            obj:Destroy()
        end
    end
end

   end,
})


Rayfield:Notify({
   Title = "Hello",
   Content = "Thank you for using it",
   Duration = 6.5,
   Image = 4483362458,
})
