-- Starting values
local radius = 200
local numShots = 500
local maxRadius = 200
local maxNumShots = 10000

-- Toggle to enable/disable the script
local enabled = false

local canShoot = true  -- Add a flag to control the shooting rate

local function updateScriptPosition()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()  -- Wait for character to load
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")  -- Wait for HumanoidRootPart to load

    for i = 1, numShots do
        if not enabled then
            break  -- Exit the loop if the script is disabled
        end
        
        -- Randomly generate x, y, z offset within a range
        local randomOffset = Vector3.new(
            math.random(-radius, radius),  -- Random x within range
            math.random(-radius, radius),  -- Random y within range (optional for height)
            math.random(-radius, radius)   -- Random z within range
        )
        
        local targetPosition = humanoidRootPart.Position + randomOffset

        local args = {
            [1] = targetPosition,
            [2] = targetPosition + Vector3.new(0, 0, 0)
        }

        -- Fire the Remote
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ZombieRelated"):WaitForChild("AcidSpit"):FireServer(unpack(args))

        task.wait(0)  -- Adjust this value to make the shots faster (lower value = faster)
    end
end

local function toggle()
    enabled = not enabled
    if enabled then
        print("Esas")
        -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏õ‡∏¥‡∏î
        Rayfield:Notify({
            Title = "Script Enabled",
            Content = "The script is now running.",
            Duration = 6.5,
            Image = 4483362458,
        })
    else
        print("coff!")
        -- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏õ‡∏¥‡∏î
        Rayfield:Notify({
            Title = "Script Disabled",
            Content = "The script has been disabled.",
            Duration = 6.5,
            Image = 4483362458,
        })
    end
end

-- Loop to update and shoot (continually checks if enabled)
task.spawn(function()
    while true do
        if enabled then
            updateScriptPosition()
        end
        task.wait(0)  -- Wait for 2 seconds before next action (can adjust as needed)
    end
end)

-- Listen for character respawn (to ensure the script works after death)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
    print("CE")
    if enabled then
        updateScriptPosition()  -- Call the function again when character respawns
    end
end)

