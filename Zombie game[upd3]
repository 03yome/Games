local Input = Tab:CreateInput({
    Name = "Summon Count ‚ö†Ô∏èWARNING‚ö†Ô∏è SHOULD NOT EXCEED 50",
    CurrentValue = "",
    PlaceholderText = "Enter number of zombies",
    RemoveTextAfterFocusLost = false,
    Flag = "Input1",
    Callback = function(Text)
        local number = tonumber(Text)
        if number and number > 0 then
            summonCount = number  -- Save inputted number globally
            Rayfield:Notify({
                Title = "Success",
                Content = "Set summon count to " .. number,
                Duration = 2,
                Image = 4483362458,
                Actions = { }
            })
        else
            Rayfield:Notify({
                Title = "Error",
                Content = "Please enter a valid number!",
                Duration = 2,
                Image = 4483362458,
                Actions = { }
            })
        end
    end
})

local Button = Tab:CreateButton({
    Name = "Summon Zombies",
    Callback = function()
        if summonCount > 0 then
            for i = 1, summonCount do
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("ZombieRelated"):WaitForChild("Necro"):WaitForChild("AbilityPlayer"):FireServer()
                task.wait(0)
            end
            Rayfield:Notify({
                Title = "Summoning...",
                Content = "Summoned " .. summonCount .. " zombies!",
                Duration = 2,
                Image = 4483362458,
                Actions = { }
            })
        else
            Rayfield:Notify({
                Title = "Error",
                Content = "Please set a summon count first!",
                Duration = 2,
                Image = 4483362458,
                Actions = { }
            })
        end
    end
})

