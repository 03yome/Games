local Tab = Window:CreateTab("Items", nil)

local Button = Tab:CreateButton({
   Name = "Give all Signs",
   Callback = function()
-- executor compatibility
local fireClick = fireclickdetector or click_detector or function() end
local getChildren = getchildren or function(obj) return obj:GetChildren() end

local lplr = game:GetService("Players").LocalPlayer
local char = lplr.Character or lplr.CharacterAdded:Wait()
local Teams = game:GetService("Teams")

-- signs c·∫ßn check
local signsToCheck
if lplr.Team == Teams.Human then
    signsToCheck = {
        "AntiZombieSign",
        "AntiZombieSign2",
        "AntiZombieSign3"
    }
else
    signsToCheck = {
        "AntiZombieSign",
        "AntiZombieSign2",
        "AntiZombieSign3",
        "ZombieSign",
        "ZombieSign2"
    }
end

-- check ƒë√£ c√≥ sign ch∆∞a
for _, name in ipairs(signsToCheck) do
    if lplr:FindFirstChild(name, true) or char:FindFirstChild(name, true) then
        warn("You already have every sign.")
        return
    end
end

-- give signs
local interaction = workspace:FindFirstChild("Interaction")
if not interaction then return end

local givers = interaction:FindFirstChild("ToolGivers")
if givers then
    for _, name in ipairs({
        "AntiZombieSign",
        "AntiZombieSign2",
        "AntiZombieSign3"
    }) do
        local giver = givers:FindFirstChild(name)
        if giver and giver:FindFirstChild("ClickDetector") then
            fireClick(giver.ClickDetector, 0)
        end
    end
end

local zombieSign = interaction:FindFirstChild("ZombieSign")
if zombieSign and zombieSign:FindFirstChild("ClickDetector") then
    fireClick(zombieSign.ClickDetector, 0)
end

-- fallback legacy
local children = getChildren(interaction)
for _, idx in ipairs({15, 16}) do
    local obj = children[idx]
    if obj and obj:FindFirstChild("ClickDetector") then
        fireClick(obj.ClickDetector, 0)
        break
    end
end

   end,
})

local Button = Tab:CreateButton({
   Name = "Rainbow Potion",
   Callback = function()
fireclickdetector(workspace.Interaction.ToolGivers["Rainbow Potion"].ClickDetector, 0)
   end,
})

local Button = Tab:CreateButton({
   Name = "Antidote",
   Callback = function()
fireclickdetector(workspace.Interaction.ToolGivers.Antidote.ClickDetector, 0)
   end,
})

local Tab = Window:CreateTab("Setting‚öôÔ∏è", nil)
local Section = Tab:CreateSection("Setting")

local UserInputService = game:GetService("UserInputService")
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
local whiteScreen = Instance.new("Frame")

screenGui.Parent = player.PlayerGui
whiteScreen.Size = UDim2.new(1, 0, 1, 0)  -- ‡∏Ñ‡∏£‡∏≠‡∏ö‡∏Ñ‡∏•‡∏∏‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠
whiteScreen.BackgroundColor3 = Color3.fromRGB(255, 255, 255)  -- ‡∏™‡∏µ‡∏Ç‡∏≤‡∏ß
whiteScreen.BackgroundTransparency = 0  -- ‡πÑ‡∏°‡πà‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™
whiteScreen.Visible = false  

whiteScreen.Parent = screenGui

local function toggleWhiteScreen()
    whiteScreen.Visible = not whiteScreen.Visible  -- ‡∏™‡∏•‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÅ‡∏™‡∏î‡∏á Notify
local function showNotify(title, content)
    Rayfield:Notify({
        Title = title,
        Content = content,
        Duration = 3,  -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏™‡∏î‡∏á Notify 3 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
        Image = 4483362458,
    })
end

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if not gameProcessedEvent then  -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏Å‡∏°‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ‡πÉ‡∏ä‡πâ‡∏õ‡∏∏‡πà‡∏°‡∏ô‡∏µ‡πâ‡∏≠‡∏¢‡∏π‡πà
        if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.P then
            toggleWhiteScreen()  -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î P
        end
    end
end)

local Toggle = Tab:CreateToggle({
    Name = "White screen (suitable for Server Crash/Auto farm)",  -- ‡∏ä‡∏∑‡πà‡∏≠ Toggle ‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÅ‡∏™‡∏î‡∏á‡πÉ‡∏ô UI
    CurrentValue = false,  -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Toggle ‡∏õ‡∏¥‡∏î
    Flag = "ToggleWhiteScreen",  -- Flag ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏Ñ‡πà‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÜ ‡πÉ‡∏ô config file
    Callback = function(Value)
        if Value then
            -- ‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡∏¥‡∏î Toggle
            print("NNN")

            showNotify("White Screen Actived", "White Screen Actived!")

            toggleWhiteScreen()
        else
            print("Toggle")

            showNotify("White Screen Deactived", "White Screen Deactived")

            toggleWhiteScreen()
        end
    end,
})

