local Tab = Window:CreateTab("Main", nil)

local running1 = false

local Toggle = Tab:CreateToggle({
   Name = "Auto Hunger",
   CurrentValue = false,
   Flag = "Toggle_Hunger",
   Callback = function(Value)
      running = Value

      if running then
         task.spawn(function()
            while running do
               local args = {
                  true,
                  1000
               }

               game:GetService("ReplicatedStorage")
                  :WaitForChild("Remotes")
                  :WaitForChild("Hunger")
                  :FireServer(unpack(args))

               task.wait(10) -- chá»‰nh tá»‘c Ä‘á»™ spam táº¡i Ä‘Ã¢y
            end
         end)
      end
   end,
})

