local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("the ayuwoki")

local KillingCheats = PhantomForcesWindow:NewSection("main")

KillingCheats:CreateButton("inf stamina ", function()
--[[
	WARNING: Heads up! This script has not been verified by ScriptBlox. Use at your own risk!
]]
while wait(0.1) do
local args = {
    [1] = 100
}

firesignal(game:GetService("ReplicatedStorage").Events.AddStamina.OnClientEvent, unpack(args))
end
print("HI")
end)

KillingCheats:CreateButton("full bright ", function()
--[[
	WARNING: Heads up! This script has not been verified by ScriptBlox. Use at your own risk!
]]
if not _G.FullBrightExecuted then

	_G.FullBrightEnabled = false

	_G.NormalLightingSettings = {
		Brightness = game:GetService("Lighting").Brightness,
		ClockTime = game:GetService("Lighting").ClockTime,
		FogEnd = game:GetService("Lighting").FogEnd,
		GlobalShadows = game:GetService("Lighting").GlobalShadows,
		Ambient = game:GetService("Lighting").Ambient
	}

	game:GetService("Lighting"):GetPropertyChangedSignal("Brightness"):Connect(function()
		if game:GetService("Lighting").Brightness ~= 1 and game:GetService("Lighting").Brightness ~= _G.NormalLightingSettings.Brightness then
			_G.NormalLightingSettings.Brightness = game:GetService("Lighting").Brightness
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").Brightness = 1
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("ClockTime"):Connect(function()
		if game:GetService("Lighting").ClockTime ~= 12 and game:GetService("Lighting").ClockTime ~= _G.NormalLightingSettings.ClockTime then
			_G.NormalLightingSettings.ClockTime = game:GetService("Lighting").ClockTime
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").ClockTime = 12
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("FogEnd"):Connect(function()
		if game:GetService("Lighting").FogEnd ~= 786543 and game:GetService("Lighting").FogEnd ~= _G.NormalLightingSettings.FogEnd then
			_G.NormalLightingSettings.FogEnd = game:GetService("Lighting").FogEnd
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").FogEnd = 786543
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("GlobalShadows"):Connect(function()
		if game:GetService("Lighting").GlobalShadows ~= false and game:GetService("Lighting").GlobalShadows ~= _G.NormalLightingSettings.GlobalShadows then
			_G.NormalLightingSettings.GlobalShadows = game:GetService("Lighting").GlobalShadows
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").GlobalShadows = false
		end
	end)

	game:GetService("Lighting"):GetPropertyChangedSignal("Ambient"):Connect(function()
		if game:GetService("Lighting").Ambient ~= Color3.fromRGB(178, 178, 178) and game:GetService("Lighting").Ambient ~= _G.NormalLightingSettings.Ambient then
			_G.NormalLightingSettings.Ambient = game:GetService("Lighting").Ambient
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
		end
	end)

	game:GetService("Lighting").Brightness = 1
	game:GetService("Lighting").ClockTime = 12
	game:GetService("Lighting").FogEnd = 786543
	game:GetService("Lighting").GlobalShadows = false
	game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)

	local LatestValue = true
	spawn(function()
		repeat
			wait()
		until _G.FullBrightEnabled
		while wait() do
			if _G.FullBrightEnabled ~= LatestValue then
				if not _G.FullBrightEnabled then
					game:GetService("Lighting").Brightness = _G.NormalLightingSettings.Brightness
					game:GetService("Lighting").ClockTime = _G.NormalLightingSettings.ClockTime
					game:GetService("Lighting").FogEnd = _G.NormalLightingSettings.FogEnd
					game:GetService("Lighting").GlobalShadows = _G.NormalLightingSettings.GlobalShadows
					game:GetService("Lighting").Ambient = _G.NormalLightingSettings.Ambient
				else
					game:GetService("Lighting").Brightness = 1
					game:GetService("Lighting").ClockTime = 12
					game:GetService("Lighting").FogEnd = 786543
					game:GetService("Lighting").GlobalShadows = false
					game:GetService("Lighting").Ambient = Color3.fromRGB(178, 178, 178)
				end
				LatestValue = not LatestValue
			end
		end
	end)
end

_G.FullBrightExecuted = true
_G.FullBrightEnabled = not _G.FullBrightEnabled
print("HI")
end)

KillingCheats:CreateButton("anti Damage fall ", function()
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local folder = ReplicatedStorage:FindFirstChild("Events") -- ⭐ Đặt tên folder tại đây
local remoteName = "TakeDamage" -- ⭐ Tên remote cần xóa

if folder then
    for _, obj in pairs(folder:GetChildren()) do
        if obj.Name == remoteName and obj:IsA("RemoteEvent") then
            obj:Destroy()
            print("Đã xóa:", obj:GetFullName())
        end
    end
else
    warn("❌ Không tìm thấy folder:", "Events")
end

print("HI")
end)

KillingCheats:CreateButton("esp render", function()
--========================================================--
--==      TOGGLE ESP MODEL + CẢNH BÁO 3 MỨC KHOẢNG CÁCH  ==--
--========================================================--

getgenv().ESP_ENABLED = not getgenv().ESP_ENABLED
if not getgenv().ESP_ENABLED then
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("BoxHandleAdornment") and v.Name == "ModelESP" then
            v:Destroy()
        end
    end
    if game.CoreGui:FindFirstChild("WarnGui") then
        game.CoreGui.WarnGui:Destroy()
    end
    print("ESP OFF")
    return
end

print("ESP ON")

local TargetNames = { "Ayuwoki" }

-- Gui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "WarnGui"
ScreenGui.Parent = game.CoreGui

local Text = Instance.new("TextLabel")
Text.Size = UDim2.new(0, 400, 0, 50)
Text.Position = UDim2.new(0.5, -200, 0.08, 0)
Text.BackgroundTransparency = 1
Text.TextScaled = true
Text.Font = Enum.Font.GothamBold
Text.Text = ""
Text.Visible = false
Text.Parent = ScreenGui

-- Kiểm tên mục tiêu
local function isTarget(name)
    for _, n in ipairs(TargetNames) do
        if n == name then return true end
    end
    return false
end

-- ESP Box
local function createESP(model)
    local p = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart")
    if not p then return end

    for _, v in ipairs(model:GetChildren()) do
        if v:IsA("BoxHandleAdornment") and v.Name == "ModelESP" then
            v:Destroy()
        end
    end

    local box = Instance.new("BoxHandleAdornment")
    box.Name = "ModelESP"
    box.Adornee = p
    box.Size = model:GetExtentsSize()
    box.Color3 = Color3.new(0, 1, 0)
    box.Transparency = 0.3
    box.AlwaysOnTop = true
    box.ZIndex = 5
    box.Parent = model
end

-- Loop update
local RunService = game:GetService("RunService")
local lp = game.Players.LocalPlayer

RunService.RenderStepped:Connect(function()
    if not getgenv().ESP_ENABLED then return end

    local char = lp.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    local nearestDist = math.huge
    local nearestModel = nil

    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and isTarget(obj.Name) then
            
            createESP(obj)

            local part = obj.PrimaryPart or obj:FindFirstChildWhichIsA("BasePart")
            if part then
                local dist = (hrp.Position - part.Position).Magnitude
                if dist < nearestDist then
                    nearestDist = dist
                    nearestModel = obj
                end
            end
        end
    end

    --===================================--
    --==  CẢNH BÁO 3 MÀU (ĐỎ - VÀNG - XANH) ==
    --===================================--

    if nearestModel and nearestDist then

        if nearestDist <= 101 then
            Text.Text = "⚠ NGUY HIỂM – RẤT GẦN ("..math.floor(nearestDist)..")"
            Text.TextColor3 = Color3.fromRGB(255, 0, 0) -- ĐỎ
            Text.Visible = true

        elseif nearestDist <= 201 then
            Text.Text = "⚠ CẢNH BÁO – GẦN ("..math.floor(nearestDist)..")"
            Text.TextColor3 = Color3.fromRGB(255, 200, 0) -- VÀNG
            Text.Visible = true

        elseif nearestDist <= 550 then
            Text.Text = "✔ AN TOÀN – XA ("..math.floor(nearestDist)..")"
            Text.TextColor3 = Color3.fromRGB(0, 255, 0) -- XANH LÁ
            Text.Visible = true

        else
            Text.Visible = false
        end

    else
        Text.Visible = false
    end
end)

print("HI")
end)

KillingCheats:CreateButton("menu flashbang ", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local StarterGui = game:GetService("StarterGui")
local player = Players.LocalPlayer

local camaraToolName = "Camara"
local target = nil
local active = false

StarterGui:SetCore("SendNotification", {
    Title = "FLASH BANG";
    Text = "cracked by romd3976";
    Duration = 3;
})

local screenGui = player:WaitForChild("PlayerGui"):FindFirstChild("FlashBangGui")
if not screenGui then
    screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FlashBangGui"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = player:WaitForChild("PlayerGui")

    local frame = Instance.new("Frame")
    frame.Name = "MainFrame"
    frame.Size = UDim2.new(0, 250, 0, 400)
    frame.Position = UDim2.new(0.5, -125, 0.5, -200)
    frame.BackgroundColor3 = Color3.new(1, 1, 1)
    frame.AnchorPoint = Vector2.new(0.5, 0.5)
    frame.Active = true
    frame.Draggable = true
    frame.Parent = screenGui

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Position = UDim2.new(0, 0, 0, 0)
    title.BackgroundTransparency = 1
    title.Text = "FLASH BANG"
    title.TextColor3 = Color3.new(0, 0, 0)
    title.TextScaled = true
    title.Parent = frame

    local stopBtn = Instance.new("TextButton")
    stopBtn.Size = UDim2.new(0, 80, 0, 30)
    stopBtn.Position = UDim2.new(1, -85, 0, 0)
    stopBtn.Text = "STOP"
    stopBtn.BackgroundColor3 = Color3.new(1, 0, 0)
    stopBtn.TextColor3 = Color3.new(1, 1, 1)
    stopBtn.Parent = frame
    stopBtn.MouseButton1Click:Connect(function()
        active = false
    end)

    local scrollingFrame = Instance.new("ScrollingFrame")
    scrollingFrame.Name = "PlayerList"
    scrollingFrame.Size = UDim2.new(1, -10, 1, -40)
    scrollingFrame.Position = UDim2.new(0, 5, 0, 35)
    scrollingFrame.BackgroundTransparency = 1
    scrollingFrame.ScrollBarThickness = 6
    scrollingFrame.Parent = frame
end

local frame = screenGui:WaitForChild("MainFrame")
local scrollingFrame = frame:WaitForChild("PlayerList")

local function findLoadCharacter()
    return ReplicatedStorage:FindFirstChild("LoadCharacter", true)
end

local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")
player.CharacterAdded:Connect(function(newChar)
    char = newChar
    hrp = char:WaitForChild("HumanoidRootPart")
end)

local function updateList()
    scrollingFrame:ClearAllChildren()
    local y = 0
    for _, plr in pairs(Players:GetPlayers()) do
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 30)
        btn.Position = UDim2.new(0, 0, 0, y)
        btn.Text = plr.Name
        btn.BackgroundColor3 = Color3.new(0.8, 0.8, 0.8)
        btn.TextColor3 = Color3.new(0, 0, 0)
        btn.Parent = scrollingFrame
        y = y + 35
        btn.MouseButton1Click:Connect(function()
            if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                target = plr.Character.HumanoidRootPart
                active = true
                StarterGui:SetCore("SendNotification", {
                    Title = "FLASH BANG";
                    Text = "Activated on "..plr.Name;
                    Duration = 3;
                })
            end
        end)
    end
    local ayu = workspace:FindFirstChild("Ayuwoki")
    if ayu and ayu:FindFirstChild("HumanoidRootPart") then
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 30)
        btn.Position = UDim2.new(0, 0, 0, y)
        btn.Text = "Ayuwoki"
        btn.BackgroundColor3 = Color3.new(1, 0, 1)
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Parent = scrollingFrame
        btn.MouseButton1Click:Connect(function()
            target = ayu.HumanoidRootPart
            active = true
            StarterGui:SetCore("SendNotification", {
                Title = "FLASH BANG";
                Text = "Activated on Ayuwoki";
                Duration = 3;
            })
        end)
        y = y + 35
    end
    scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, y)
end

Players.PlayerAdded:Connect(updateList)
Players.PlayerRemoving:Connect(updateList)
updateList()

spawn(function()
    while true do
        RunService.RenderStepped:Wait()
        if active and target and char and hrp then
            local lookVector = target.CFrame.LookVector
            local frontPos = target.Position + lookVector * 20
            hrp.CFrame = CFrame.new(frontPos, target.Position)
            workspace.CurrentCamera.CFrame = CFrame.new(hrp.Position, target.Position)
            local tool = player.Backpack:FindFirstChild(camaraToolName) or char:FindFirstChild(camaraToolName)
            if tool then
                char.Humanoid:EquipTool(tool)
                if tool:FindFirstChild("Activate") then
                    tool.Activate:FireServer()
                else
                    tool:Activate()
                end
                wait(2)
                local loadChar = findLoadCharacter()
                if loadChar then
                    if loadChar:IsA("RemoteEvent") then
                        loadChar:FireServer()
                    elseif loadChar:IsA("RemoteFunction") then
                        loadChar:InvokeServer()
                    end
                end
            end
            wait(0.5)
        end
    end
end)
print("HI")
end)
