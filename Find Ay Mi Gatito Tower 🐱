local Section = Tab:CreateSection("Coin Collection")

Tab:CreateButton({
    Name = "Touch All Coins",
    Callback = function()
        local Players = game:GetService("Players")
        local lp = Players.LocalPlayer
        local char = lp.Character or lp.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        local CoinsFolder = workspace:WaitForChild("Map"):WaitForChild("Coins")
        local touchedCount = 0

        -- Notify b·∫Øt ƒë·∫ßu
        Rayfield:Notify({
            Title = "Starting",
            Content = "Touching all coins...",
            Duration = 2
        })

        for _, coin in ipairs(CoinsFolder:GetChildren()) do
            local part

            if coin:IsA("BasePart") then
                part = coin
            elseif coin:IsA("Model") then
                part = coin:FindFirstChildWhichIsA("BasePart")
            end

            if part then
                -- Tele t·ªõi coin
                hrp.CFrame = part.CFrame + Vector3.new(0, 3, 0)

                -- Touch coin
                firetouchinterest(hrp, part, 0)
                task.wait()
                firetouchinterest(hrp, part, 1)

                touchedCount += 1
                task.wait(0.01)
            end
        end

        -- Notify k·∫øt th√∫c
        Rayfield:Notify({
            Title = "Complete",
            Content = "Touched " .. touchedCount .. " coins",
            Duration = 3
        })
    end
})

