local Section = Tab:CreateSection("Add Spins")

local Input = Tab:CreateInput({
    Name = "Custom Spin Amount",
    CurrentValue = "",
    PlaceholderText = "Enter number of spins",
    RemoveTextAfterFocusLost = false,
    Flag = "InputSpin",
    Callback = function(Text)
        local amount = tonumber(Text)
        if not amount then
            Rayfield:Notify({
                Title = "L·ªói",
                Content = "Vui l√≤ng nh·∫≠p s·ªë h·ª£p l·ªá!",
                Duration = 3
            })
            return
        end

        local args = {
            [1] = amount
        }

        game:GetService("ReplicatedStorage")
            .RemoteEvents
            .SpinAdd
            :FireServer(unpack(args))

        Rayfield:Notify({
            Title = "Success",
            Content = "Added " .. amount .. " spin!",
            Duration = 3
        })
    end,
})

