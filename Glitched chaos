-- âš™ï¸ FE Kill Aura + Gamepass + Godmode (No Key, Full Combine)
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")

-- ðŸ–¥ï¸ GUI chÃ­nh
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
ScreenGui.Name = "KillAuraFullGui"

-- cho phÃ©p báº­t/táº¯t báº±ng Ctrl
local toggleVisible = true
game:GetService("UserInputService").InputBegan:Connect(function(input, processed)
	if not processed and input.KeyCode == Enum.KeyCode.LeftControl then
		toggleVisible = not toggleVisible
		ScreenGui.Enabled = toggleVisible
	end
end)

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 300, 0, 250)
Frame.Position = UDim2.new(0.5, -150, 0.5, -125)
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.Active = true
Frame.Draggable = true
Instance.new("UICorner", Frame).CornerRadius = UDim.new(0, 10)

local Stroke = Instance.new("UIStroke", Frame)
Stroke.Thickness = 2
Stroke.Color = Color3.fromRGB(0, 255, 255)

local Title = Instance.new("TextLabel", Frame)
Title.Text = "â­ FE Kill Aura + Gamepass â­"
Title.Size = UDim2.new(1, 0, 0, 35)
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.new(1, 1, 1)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 20

-- ðŸ§® Nháº­p khoáº£ng cÃ¡ch
local DistanceBox = Instance.new("TextBox", Frame)
DistanceBox.PlaceholderText = "Nháº­p khoáº£ng cÃ¡ch (VD: 15)"
DistanceBox.Size = UDim2.new(1, -20, 0, 35)
DistanceBox.Position = UDim2.new(0, 10, 0, 45)
DistanceBox.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
DistanceBox.TextColor3 = Color3.new(1, 1, 1)
DistanceBox.Font = Enum.Font.Gotham
DistanceBox.TextSize = 16
Instance.new("UICorner", DistanceBox).CornerRadius = UDim.new(0, 8)

-- ðŸ—¡ï¸ NÃºt báº­t Kill Aura
local KillBtn = Instance.new("TextButton", Frame)
KillBtn.Text = "Báº­t Kill Aura"
KillBtn.Size = UDim2.new(1, -20, 0, 40)
KillBtn.Position = UDim2.new(0, 10, 0, 95)
KillBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
KillBtn.TextColor3 = Color3.new(1, 1, 1)
KillBtn.Font = Enum.Font.GothamBold
KillBtn.TextSize = 18
Instance.new("UICorner", KillBtn).CornerRadius = UDim.new(0, 8)

-- âš¡ NÃºt báº­t Gamepass FE + Godmode
local GamepassBtn = Instance.new("TextButton", Frame)
GamepassBtn.Text = "Báº­t FE Gamepass + Godmode"
GamepassBtn.Size = UDim2.new(1, -20, 0, 40)
GamepassBtn.Position = UDim2.new(0, 10, 0, 145)
GamepassBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
GamepassBtn.TextColor3 = Color3.new(1, 1, 1)
GamepassBtn.Font = Enum.Font.GothamBold
GamepassBtn.TextSize = 18
Instance.new("UICorner", GamepassBtn).CornerRadius = UDim.new(0, 8)

-- âš ï¸ Label hÆ°á»›ng dáº«n
local Hint = Instance.new("TextLabel", Frame)
Hint.Text = "áº¤n Ctrl Ä‘á»ƒ áº©n/hiá»‡n GUI"
Hint.Size = UDim2.new(1, 0, 0, 20)
Hint.Position = UDim2.new(0, 0, 0, 200)
Hint.BackgroundTransparency = 1
Hint.TextColor3 = Color3.fromRGB(150,150,150)
Hint.Font = Enum.Font.Gotham
Hint.TextSize = 14

-- ðŸ”˜ Tráº¡ng thÃ¡i
local auraEnabled = false
local godmodeEnabled = false
local distance = 15

-------------------------------------------------------
-- ðŸ§  Logic Kill Aura
KillBtn.MouseButton1Click:Connect(function()
	if auraEnabled then
		auraEnabled = false
		KillBtn.Text = "Báº­t Kill Aura"
		KillBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
	else
		local disVal = tonumber(DistanceBox.Text)
		if not disVal then
			DistanceBox.Text = "âš ï¸ Nháº­p sá»‘ há»£p lá»‡!"
			return
		end
		distance = disVal
		auraEnabled = true
		KillBtn.Text = "Táº¯t Kill Aura"
		KillBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)

		task.spawn(function()
			while auraEnabled do
				task.wait(0.1)
				for _, v in pairs(Workspace:WaitForChild("NPC DAMAGE"):GetDescendants()) do
					local humanoid = v:FindFirstChildOfClass("Humanoid")
					local rootPart = v:FindFirstChild("HumanoidRootPart")
					if humanoid and rootPart and humanoid.Health > 0 and v.Name ~= LocalPlayer.Name then
						local myRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
						if myRoot then
							local dist = (myRoot.Position - rootPart.Position).Magnitude
							if dist <= distance then
								pcall(function()
									repeat
										task.wait()
										humanoid.Health = 0
										rootPart.CanCollide = false
										sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
									until humanoid.Health <= 0 or not auraEnabled
								end)
							end
						end
					end
				end
			end
		end)
	end
end)

-------------------------------------------------------
-- ðŸ›¡ï¸ Logic Gamepass FE + Godmode (gá»™p code cÅ©)
GamepassBtn.MouseButton1Click:Connect(function()
	godmodeEnabled = not godmodeEnabled
	if godmodeEnabled then
		GamepassBtn.Text = "Äang kÃ­ch hoáº¡t FE + Godmode"
		GamepassBtn.BackgroundColor3 = Color3.fromRGB(255, 100, 0)
		task.spawn(function()
			while godmodeEnabled do
				wait(0.1)
				for _, v in pairs(Workspace:WaitForChild("NPC DAMAGE"):GetDescendants()) do
					local humanoid = v:FindFirstChildOfClass("Humanoid")
					local rootPart = v:FindFirstChild("HumanoidRootPart")
					if humanoid and rootPart and humanoid.Health > 0 and v.Name ~= LocalPlayer.Name then
						local distance = (LocalPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
						if distance <= 25 then
							pcall(function()
								repeat
									wait()
									humanoid.Health = 0
									rootPart.CanCollide = false
									sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
								until humanoid.Health <= 0 or not godmodeEnabled
							end)
						end
					end
				end
				pcall(function()
					sethiddenproperty(LocalPlayer, "SimulationRadius", math.huge)
					local char = LocalPlayer.Character
					if char then
						for _, part in pairs(char:GetDescendants()) do
							if part:IsA("BasePart") then
								part.CanTouch = false
								part.CanCollide = false
								part.Anchored = false
							end
						end
					end
				end)
			end
		end)
	else
		GamepassBtn.Text = "Báº­t FE Gamepass + Godmode"
		GamepassBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
	end
end)
