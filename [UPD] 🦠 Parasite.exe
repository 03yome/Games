local Tab = Window:CreateTab("Kill monters", 4483362458)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local OrganismsFolder = workspace:WaitForChild("Organisms")

local SelectedTargetName = nil
local DamageValue = 25 -- m·∫∑c ƒë·ªãnh

-- INPUT DAMAGE
local Input = Tab:CreateInput({
    Name = "Damage",
    CurrentValue = "",
    PlaceholderText = "Enter Damage...",
    RemoveTextAfterFocusLost = false,
    Flag = "DamageInput",
    Callback = function(Text)
        local number = tonumber(Text)
        if number then
            DamageValue = number
        end
    end,
})

-- L·∫§Y DANH S√ÅCH M·ª§C TI√äU TRONG ORGANISMS
local targetNames = {}

for _, v in pairs(OrganismsFolder:GetChildren()) do
    table.insert(targetNames, v.Name)
end

-- DROPDOWN CH·ªåN T√äN
local Dropdown = Tab:CreateDropdown({
    Name = "Select Target",
    Options = targetNames,
    CurrentOption = {},
    MultipleOptions = false,
    Flag = "TargetDropdown",
    Callback = function(Options)
        SelectedTargetName = Options[1]
    end,
})

-- H√ÄM T√åM TARGET G·∫¶N NH·∫§T (n·∫øu tr√πng t√™n)
local function GetClosestTargetByName(name)
    local closest = nil
    local shortestDistance = math.huge

    for _, v in pairs(OrganismsFolder:GetChildren()) do
        if v.Name == name then
            local humanoidRoot = v:FindFirstChild("HumanoidRootPart")
            if humanoidRoot then
                local distance = (LocalPlayer.Character.HumanoidRootPart.Position - humanoidRoot.Position).Magnitude

                if distance < shortestDistance then
                    shortestDistance = distance
                    closest = v
                end
            end
        end
    end

    return closest
end

-- BUTTON B·∫ÆN
local Button = Tab:CreateButton({
    Name = "Fire",
    Callback = function()
        if not SelectedTargetName then return end

        local target = GetClosestTargetByName(SelectedTargetName)
        if not target then return end

        local humanoid = target:FindFirstChild("Humanoid")
        local root = target:FindFirstChild("HumanoidRootPart")

        if humanoid and root then
            local args = {
                humanoid,
                root,
                DamageValue,
                Vector3.new(0.6275, 0.2666, 0.7315),
                0,
                0,
                false
            }

            LocalPlayer.Character
                :WaitForChild("AR")
                :WaitForChild("GunScript_Server")
                :WaitForChild("InflictTarget")
                :FireServer(unpack(args))
        end
    end
})
